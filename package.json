{
  "name": "eslint-plugin-arrow-return-style",
  "version": "1.3.0",
  "description": "Enforce arrow function return style and automatically fix it",
  "keywords": [
    "arrow-body-style",
    "arrow-return-style",
    "arrow-function"
  ],
  "repository": {
    "url": "https://github.com/u3u/eslint-plugin-arrow-return-style.git"
  },
  "license": "MIT",
  "author": "u3u <qwq@qwq.cat> (https://github.com/u3u)",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && fix-tsup-cjs",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "prepare": "pnpm build",
    "test": "vitest --ui --coverage.enabled --coverage.all=false",
    "update:eslint-docs": "pnpm build && eslint-doc-generator"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --concurrent false"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,jsx,ts,tsx,md}": [
      "eslint --fix"
    ],
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@typescript-eslint/utils": "^6.21.0",
    "scule": "^1.3.0"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.2",
    "@typescript-eslint/rule-tester": "^6.21.0",
    "@u3u/eslint-config": "^2.15.1",
    "@u3u/prettier-config": "^4.5.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "dedent": "^1.5.3",
    "eslint": "^8.57.0",
    "eslint-doc-generator": "^1.7.1",
    "eslint-plugin-arrow-return-style": "link:",
    "fix-tsup-cjs": "^1.2.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.1",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.8",
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "@types/node"
      ]
    }
  }
}
