{
  "name": "eslint-plugin-arrow-return-style",
  "version": "1.2.1",
  "description": "Enforce arrow function return style and automatically fix it",
  "keywords": [
    "arrow-body-style",
    "arrow-return-style",
    "arrow-function"
  ],
  "repository": {
    "url": "https://github.com/u3u/eslint-plugin-arrow-return-style.git"
  },
  "license": "MIT",
  "author": "u3u <qwq@qwq.cat> (https://github.com/u3u)",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && fix-tsup-cjs",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "prepare": "pnpm build",
    "test": "vitest --ui --coverage",
    "update:eslint-docs": "pnpm build && eslint-doc-generator"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --concurrent false"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,jsx,ts,tsx,md}": [
      "eslint --fix"
    ],
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@typescript-eslint/utils": "^6.6.0",
    "scule": "^1.0.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.1",
    "@types/node": "^18.17.14",
    "@typescript-eslint/rule-tester": "^6.6.0",
    "@u3u/eslint-config": "^2.10.0",
    "@u3u/prettier-config": "^3.1.0",
    "@vitest/coverage-v8": "^0.34.3",
    "@vitest/ui": "^0.34.3",
    "dedent": "^1.5.1",
    "eslint": "^8.48.0",
    "eslint-doc-generator": "^1.4.3",
    "eslint-plugin-arrow-return-style": "link:",
    "fix-tsup-cjs": "^1.2.0",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "packageManager": "pnpm@8.7.1",
  "publishConfig": {
    "access": "public"
  }
}
